# set PATH
PATH=/usr/local/bin:$PATH
PATH=/usr/local/share/npm/bin:$PATH
PATH=$HOME/.dot/bin:$PATH
export PATH

# set editors
export EDITOR=vim
export VISUAL=vim

export PYTHONPATH=/home/h2ometrics/h2oserver

alias sup='supervisorctl'
alias activate='source /home/h2ometrics/h2oserver-env/bin/activate'
alias ls='ls -lah'

# start on h2oserver
cd ~/h2oserver
source /home/h2ometrics/h2oserver-env/bin/activate
